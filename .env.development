# Development Environment Configuration
NODE_ENV=development
BUILD_TARGET=dev

# Ports
BACKEND_PORT=5000
FRONTEND_PORT=3000
REDIS_PORT=6379

# API Configuration
NEXT_PUBLIC_API_URL=http://localhost:5000/api

# Database (SQLite for development)
DATABASE_URL="file:./prisma/dev.db"

# Security (use weak secrets for development only)
JWT_SECRET=dev-jwt-secret-key-not-for-production
JWT_EXPIRE=7d
SESSION_SECRET=dev-session-secret-not-for-production
PUZZLE_SECRET=dev-puzzle-secret-not-for-production

# OAuth (development credentials)
GOOGLE_CLIENT_ID=your-dev-google-client-id
GOOGLE_CLIENT_SECRET=your-dev-google-client-secret

# Redis
REDIS_PASSWORD=dev-redis-password

# Auto-solve Configuration
AUTO_SOLVE_COOLDOWN_HOURS=1

# Logging
LOG_LEVEL=debug

# Development flags
ENABLE_SWAGGER=true
ENABLE_CORS_ALL=true